<Window x:Class="WpfApp5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
        <Button Content="page1" Command="{Binding NavigatePage1Command}" Margin="5"></Button>
        <Button Content="page2" Command="{Binding NavigatePage2Command}" Margin="5"></Button>
        <Button Content="Back" Command="{Binding BackCommand}" Margin="5"></Button>
        <Button Content="Forward" Command="{Binding ForwardCommand}" Margin="5"></Button>
        </StackPanel>
        <Frame x:Name="mainframe" NavigationUIVisibility="Hidden"></Frame>
    </DockPanel>
</Window>
